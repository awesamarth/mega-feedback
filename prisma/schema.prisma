generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Feedback {
  id                 String   @id @default(uuid())
  encrypted_feedback String
  category          String
  created_at        DateTime
}